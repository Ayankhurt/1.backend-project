{
    "rewrites": [{
        "source": "/(.*)",
        "destination": "/"
    }]
    "version": 2,
    "builds": [
     {
      "src": "srver.mjs",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "srver.mjs"
     }
    ]
   }